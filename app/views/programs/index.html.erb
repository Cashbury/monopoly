<h1>Listing programs</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Type</th>
    <th>Auto enroll</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Business</th>
    <th>Initial points</th>
    <th>Max points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @programs.each do |program| %>
  <tr>
    <td><%= program.name %></td>
    <td><%= program.program_type_id %></td>
    <td><%= program.auto_enroll %></td>
    <td><%= program.start_date %></td>
    <td><%= program.end_date %></td>
    <td><%= program.business_id %></td>
    <td><%= program.initial_points %></td>
    <td><%= program.max_points %></td>
    <td><%= link_to 'Show', program %></td>
    <td><%= link_to 'Edit', edit_program_path(program) %></td>
    <td><%= link_to 'Destroy', program, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Program', new_program_path %><br />
<%= link_to "Show all types", program_types_path %>